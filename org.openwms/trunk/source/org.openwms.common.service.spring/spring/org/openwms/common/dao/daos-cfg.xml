<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="locationDao" class="org.openwms.common.dao.core.LocationDaoImpl">
		<property name="entityManagerFactory" ref="entityManagerFactory" />
	</bean>

	<bean id="locationGroupDao" class="org.openwms.common.dao.core.LocationGroupDaoImpl">
		<property name="entityManagerFactory" ref="entityManagerFactory" />
	</bean>

	<bean id="userDao" class="org.openwms.common.dao.management.UserDaoImpl">
		<property name="entityManagerFactory" ref="entityManagerFactory" />
	</bean>
</beans>
