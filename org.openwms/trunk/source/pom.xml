        <profile>
            <id>public</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <!-- Web Site deployment -->
                <distribution.serverId>repo.sourceforge.net</distribution.serverId>
                <distribution.serverProtocol>scp</distribution.serverProtocol>
