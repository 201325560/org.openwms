                <distribution.repo.serverProtocol>http:</distribution.repo.serverProtocol>
                <distribution.repo.serverUrl.rootPath>rudi:8081/nexus/content/repositories/releases</distribution.repo.serverUrl.rootPath>
                <distribution.repo.snapshot.serverUrl.rootPath>rudi:8081/nexus/content/repositories/snapshots</distribution.repo.snapshot.serverUrl.rootPath>
            </properties>
        </profile>
