            <id>domain</id>
            <modules>
                <module>org.openwms.common.integration</module>
            </modules>
        </profile>
        <profile>
            <id>release</id>
            <build>
    	        <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-javadoc-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>attach-javadocs</id>
                                <goals>
